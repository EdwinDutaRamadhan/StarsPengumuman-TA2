<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertPengumumanMahasiswa</servlet-name>
        <servlet-class>Service.InsertPengumumanMahasiswa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePengumumanMahasiswa</servlet-name>
        <servlet-class>Service.DeletePengumumanMahasiswa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePengumumanMahasiswa</servlet-name>
        <servlet-class>Service.UpdatePengumumanMahasiswa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectPengumuman</servlet-name>
        <servlet-class>Service.SelectPengumuman</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailPengumuman</servlet-name>
        <servlet-class>Service.DetailPengumuman</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertPengumumanMahasiswa</servlet-name>
        <url-pattern>/InsertPengumumanMahasiswa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePengumumanMahasiswa</servlet-name>
        <url-pattern>/DeletePengumumanMahasiswa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdatePengumumanMahasiswa</servlet-name>
        <url-pattern>/UpdatePengumumanMahasiswa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectPengumuman</servlet-name>
        <url-pattern>/SelectPengumuman</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailPengumuman</servlet-name>
        <url-pattern>/DetailPengumuman</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
